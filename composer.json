{
  "name": "tomcizek/symfony-interop-container",
  "type": "library",
  "description": "Symfony Interop Container which has config array under 'config' key - for Extensions using Interoperability Factories.",
  "license": "MIT",
  "authors": [
    {
      "name": "Tomáš Čížek",
      "email": "tomascizek@gmail.com"
    }
  ],
  "require": {
    "php": "^7.0",
    "symfony/dependency-injection": "^3.3",
    "symfony/yaml": "^3.3",
    "symfony/config": "^3.3",
    "symfony/expression-language": "^3.3",
    "symfony/http-kernel": "^3.3",
    "psr/container": "^1.0",
    "sandrokeil/interop-config": "^2.1",
    "symfony/framework-bundle": "^3.3"
  },
  "autoload": {
    "classmap": [
      "src/"
    ]
  },
  "require-dev": {
    "phpunit/phpunit": "^6.2",
    "phpstan/phpstan": "^0.8.5"
  },
  "autoload-dev": {
    "classmap": [
      "tests/"
    ]
  }
}
